extends layout

block content
  body(id="page-top")
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg bg-secondary text-uppercase fixed-top" id="mainNav">
      <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="#page-top">Ottawa LRT Status</a>
        <button class="navbar-toggler navbar-toggler-right text-uppercase font-weight-bold bg-primary text-white rounded" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          Menu
          <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger"><span id="hoursAgo">00</span>h since last incident</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Report">Report</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Latest">Latest</a>
            </li>
            <li class="nav-item mx-0 mx-lg-1">
              <a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="#Statistics">Statistics</a>
            </li>

          </ul>
        </div>
      </div>
    </nav>
    <!-- Masthead -->
    if currentStatus.overallStatus == "success"
      <header class="masthead bg-primary text-white text-center">
        <div class="container d-flex align-items-center flex-column">

          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_success.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is running*</h1>
          small(class="masthead-subheading mb-0") We report fast. The Mayor reports furious.

    else if currentStatus.overallStatus == "danger"
      <header class="masthead bg-danger text-white text-center">
        <div class="container d-flex align-items-center flex-column">

          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_danger.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is a hot mess right now</h1>
          small(class="masthead-subheading mb-0") #{events[0].subText}

    else if currentStatus.overallStatus == "warning"
      <header class="masthead bg-warning text-white text-center">
        <div class="container d-flex align-items-center flex-column">

          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_warning.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is struggling with life right now</h1>
          small(class="masthead-subheading mb-0") #{events[0].subText}

    else
      <header class="masthead bg-secondary text-white text-center">
        <div class="container d-flex align-items-center flex-column">
          <!-- Masthead Avatar Image -->
          <img class="masthead-avatar mb-5" src="img/hotmessexpress_info.png" alt="The Hot Mess Express">

          <!-- Masthead Heading -->
          <h1 class="masthead-heading text-uppercase mb-0">The LRT is running fine???</h1>
          small(class="masthead-subheading mb-0") We are having trouble figuring out whats going right now. <br> Check back shortly.

    <!-- Icon Divider -->
    <div class="divider-custom divider-light">
      <div class="divider-custom-line"></div>
      <div class="divider-custom-icon">
        <i class="fas fa-train"></i>
      </div>
      <div class="divider-custom-line"></div>
    </div>

    <!-- Masthead Subheading -->
    p(class="masthead-subheading mb-0") #{events[0].subtext}
    <p class="masthead-subheading mb-0">Last incident was <span id="example" data-toggle="tooltip" data-placement="top"> <b class=text-uppercase><span id="hoursAgoMain">?? </span>h</b> ago</span> at <b class=text-uppercase><span id="lastLocation">Unknown location</span></b> due to <b class=text-uppercase><span id="lastReason">Unspecified Reason</span></b></p>

    <!-- Scroll to Events -->
    <a class="btn btn-outline-light btn-social mx-1 mt-3 js-scroll-trigger" href="#activeEvents">
      <i class="fa fa-chevron-down"></i>
    </a>

      </div>
    </header>

        <!-- Copyright Section -->
    <section class="copyright py-4 text-center text-white">
      <div class="container">
        small.text-uppercase Quick station status
        p.mb-3 
        a.btn.btn-sm.btn-outline-success TP
        a.btn.btn-sm.btn-outline-success BV
        a.btn.btn-sm.btn-outline-success PIM
        a.btn.btn-sm.btn-outline-success LYN
        a.btn.btn-sm.btn-outline-success PAR
        a.btn.btn-sm.btn-outline-success RDU
        a.btn.btn-sm.btn-outline-success UOT
        a.btn.btn-sm.btn-outline-success LEE
        a.btn.btn-sm.btn-outline-success HRD
        a.btn.btn-sm.btn-outline-success TMB
        a.btn.btn-sm.btn-outline-success STL
        a.btn.btn-sm.btn-outline-success CYR
        a.btn.btn-sm.btn-outline-success BLA
        br
        button.small.btn.btn-link(href="#reports") See Details
      </div>
    </section>

    section(class="page-section" id="activeEvents")
      .container
        .row
          .col
            <!-- activeEvents Section Heading -->
            h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-5 Current Incidents
            <!-- Icon Divider -->
            <div class="divider-custom divider-dark">
              <div class="divider-custom-line"></div>
              <div class="divider-custom-icon">
                <i class="fas fa-train"></i>
              </div>
              <div class="divider-custom-line"></div>
            </div>

            <div class="accordion" id="events">
            
              <div class="card">
                <div class="card-header" style="padding-left: 0px" id="headingOne">
                  <h2 class="mb-0">
                    button(class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne")
                      span(class="btn btn-outline-primary btn-social mx-1")
                        span.small 5m
                      span(style="margin-left: 10px" class="masthead-heading text-uppercase text-secondary") Delay due to door held
                      button.btn.btn-primary.float-right Pimisi
                  </h2>
                </div>
                <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#events">
                  <div class="card-body">
                    p 2:00 am: Anim pariatur cliche reprehenderit, enim eiusmod high life accusamus
                    p 2:03 am: terry richardson ad squid.
                    p 2:15 am: wolf moon officia aute, non cupidatat skateboard dolor brunch. 
                  </div>
                </div>
              </div>

              <div class="card">
                <div class="card-header" style="padding-left: 0px" id="headingTwo">
                  <h2 class="mb-0">
                    <button class="btn btn-link collapsed" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                      a(class="btn btn-outline-primary btn-social mx-1")
                        span.small 15m
                      span(style="margin-left: 10px" class="masthead-heading text-uppercase text-secondary") Delay due to stopped train
                      button.btn.btn-primary.float-right Rideau
                      button.btn.btn-primary.float-right.mr-2 Parliament
                    </button>
                  </h2>
                </div>
                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#events">
                  <div class="card-body">
                    p 2:00 am: Anim pariatur cliche reprehenderit, enim eiusmod high life accusamus
                    p 2:03 am: terry richardson ad squid.
                    p 2:15 am: wolf moon officia aute, non cupidatat skateboard dolor brunch. 
                  </div>
                </div>
              </div>

              <div class="card">
                <div class="card-header" style="padding-left: 0px" id="headingThree">
                  <h2 class="mb-0">
                    <button class="btn btn-link collapsed" type="button" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                      a(class="btn btn-outline-warning btn-social mx-1")
                        span.small 43m
                      span(style="margin-left: 10px" class="masthead-heading text-uppercase text-secondary") Delay due to door immobilized train
                      button.btn.btn-primary.float-right Pimisi

                    </button>
                  </h2>
                </div>
                <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#events">
                  <div class="card-body">
                    p 2:00 am: Anim pariatur cliche reprehenderit, enim eiusmod high life accusamus
                    p 2:03 am: terry richardson ad squid.
                    p 2:15 am: wolf moon officia aute, non cupidatat skateboard dolor brunch. 
                  </div>
                </div>
              </div>
            </div>
        .row
          .col
            h3.text-center.mt-5 Completed Incidents

    <!-- Report Section -->
    <section class="page-section" id="Report">
      .container
        .row
          .col-lg-6
            <!-- Contact Section Heading -->
            <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Report an incident</h2>

            <!-- Icon Divider -->
            <div class="divider-custom">
              <div class="divider-custom-line"></div>
              <div class="divider-custom-icon">
                <i class="fas fa-exclamation"></i>
              </div>
              <div class="divider-custom-line"></div>
            </div>

            <!-- Report-->
            <div class="row">
              <div class="col mb-3">
                <p class="text-center lead">Send us a status report</p>
              </div>
            </div>
            <div class="row mb-5">
              <div class="col-lg-3 ml-auto">
                <div class="text-center mt-2">
                  <a class="btn btn-outline-dark" href="https://www.reddit.com/message/compose/?to=JKOttawa&subject=Reporting%20LRT%20Status&message=Location:%0ATime:%0AReason:%0A">
                    <i class="fab fa-reddit-alien"></i> Reddit
                  </a>
                </div>
              </div>
              <div class="col-lg-3 mr-auto">
                <div class="text-center mt-2">
                  <a class="btn btn-outline-dark" href="mailto:control.ottawa@gmail.com?subject=Reporting LRT Status&body=Time:%0ALocation:%0AReason:%0A%0A">
                    <i class="far fa-envelope"></i> Email
                  </a>
                </div>
              </div>
            </div>
            
          .col-lg-5
            <!-- Email Alerts -->
            <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Sign up for email alerts</h2>

            <!-- Icon Divider -->
            <div class="divider-custom">
              <div class="divider-custom-line"></div>
              <div class="divider-custom-icon">
                <i class="fas fa-envelope"></i>
              </div>
              <div class="divider-custom-line"></div>
            </div>

            <!-- Email -->
            <div class="row">
              <div class="col mb-3">
                <div class="text-center mt-2">
                  <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#emailMode">
                    button(class="btn btn-outline-dark") Signup
                  </div>
                </div>
              </div>
            </div>   

    </section>
    
    <!-- Latest Section -->
    <section class="page-section bg-primary text-white mb-0" id="Latest">
      <div class="container">

        <!-- About Section Heading -->
        <h2 class="page-section-heading text-center text-uppercase text-white">Latest Station Status</h2>

        <!-- Icon Divider -->
        <div class="divider-custom divider-light">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon">
            <i class="fas fa-star"></i>
          </div>
          <div class="divider-custom-line"></div>
        </div>

        <!-- About Section Content -->
        <div class="row">
          <div class="col-lg-6">
            <!-- Current Status per station -->
            <!-- Line 1 -->
            p.page-section-heading.text-center.text-uppercase.text-white Line 1                    
            each val, index in currentStatus.stopStatus.l1
              .mb-0.text-center
                .form-group("titleBlock")
                  p.lead 
                    <span id="example" data-toggle="tooltip" data-placement="right" data-title="#{val.tooltipText}">
                    if val.status == "danger"
                      <button class="btn btn-danger"><i class="fas fa-fire"></i> #{index}</button>
                    else if val.status == "warning"
                      <button class="btn btn-warning"><i class="fas fa-exclamation"></i> #{index}</button>
                    else if val.status == "info"
                      <button class="btn btn-info"><i class="fas fa-exclamation-circle"></i> #{index}</button>
                    else if val.status == "success"
                      <button class="btn btn-success"> #{index}</button>
                    </span>
          </div>

          <div class="col-lg-6">
            <!-- Line 2 -->
            p.page-section-heading.text-center.text-uppercase.text-white Line 2                    
            each val, index in currentStatus.stopStatus.l2
              .mb-0.text-center
                .form-group("titleBlock")
                  p.lead 
                    <span id="example" data-toggle="tooltip" data-placement="right" data-title="#{val.tooltipText}">
                    if val.status == "danger"
                      <button class="btn btn-danger"><i class="fas fa-fire"></i> #{index}</button>
                    else if val.status == "warning"
                      <button class="btn btn-warning"><i class="fas fa-exclamation"></i> #{index}</button>
                    else if val.status == "info"
                      <button class="btn btn-info"><i class="fas fa-exclamation-circle"></i> #{index}</button>
                    else if val.status == "success"
                      <button class="btn btn-success"> #{index}</button>
                    </span>
          </div>
        </div>
        <!-- -->
        <div class="text-center mt-4">
        </div>

      </div>
    </section>

    <!-- Stats Section -->
    <section class="page-section portfolio" id="Statistics">
      <div class="container">

        <!-- Stats Section Heading -->
        <h2 class="page-section-heading text-center text-uppercase text-secondary mb-0">Statistics</h2>

        <!-- Icon Divider -->
        <div class="divider-custom">
          <div class="divider-custom-line"></div>
          <div class="divider-custom-icon">
            <i class="fas fa-train"></i>
          </div>
          <div class="divider-custom-line"></div>
        </div>

        <!-- Stats Grid Items -->
        <div class="page-section-heading">
          h3.text-center A work in progress 
            small.text-primary (just like the LRT)
        .row.mt-5
          <!-- Portfolio Item 1 -->
          <div class="col-md-6 col-lg-4">
            <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#recordModal">
              <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                <div class="portfolio-item-caption-content text-center text-white">
                    p Records
                </div>
              </div>
              p DAYS WITHOUT INCIDENT STREAK
              p -25 DAYS
       
            </div>
          </div>

           <!-- Portfolio Item 2 -->
          <div class="col-md-6 col-lg-4">
            <div class="portfolio-item mx-auto" data-toggle="modal" data-target="#chartModal">
              <div class="portfolio-item-caption d-flex align-items-center justify-content-center h-100 w-100">
                <div class="portfolio-item-caption-content text-center text-white">
                    p Charts
                </div>
              </div>
              <canvas id="myChart" width="400" height="400"></canvas>
              script.
                var ctx = document.getElementById('myChart').getContext('2d');
                var myChart = new Chart(ctx, {
                  type: 'bar',
                  data: {
                      labels: ['TP', 'BV', 'PIM', 'LYN', 'PAR', 'RDU', 'uOT', 'LEE', 'HUR', 'TMB', 'STL', 'CYR', 'BLR'],
                      datasets: [{
                          label: '# of Incidents',
                          data: [3, 1, 4, 1, 1, 1, 3, 2, 1, 0, 2, 0, 6],
                          backgroundColor: [
                              'rgba(255, 99, 132, 0.2)',
                              'rgba(54, 162, 235, 0.2)',
                              'rgba(255, 206, 86, 0.2)',
                              'rgba(75, 192, 192, 0.2)',
                              'rgba(153, 102, 255, 0.2)',
                              'rgba(255, 159, 64, 0.2)',
                              'rgba(255, 99, 132, 0.2)',
                              'rgba(54, 162, 235, 0.2)',
                              'rgba(255, 206, 86, 0.2)',
                              'rgba(75, 192, 192, 0.2)',
                              'rgba(153, 102, 255, 0.2)',
                              'rgba(255, 159, 64, 0.2)',
                              'rgba(255, 99, 132, 0.2)'
                          ],
                          borderColor: [
                              'rgba(255, 99, 132, 1)',
                              'rgba(54, 162, 235, 1)',
                              'rgba(255, 206, 86, 1)',
                              'rgba(75, 192, 192, 1)',
                              'rgba(153, 102, 255, 1)',
                              'rgba(255, 159, 64, 1)',
                              'rgba(255, 99, 132, 1)',
                              'rgba(54, 162, 235, 1)',
                              'rgba(255, 206, 86, 1)',
                              'rgba(75, 192, 192, 1)',
                              'rgba(153, 102, 255, 1)',
                              'rgba(255, 159, 64, 1)',
                              'rgba(255, 99, 132, 1)'
                          ],
                          borderWidth: 1
                      }]
                  },
                  options: {
                      scales: {
                          yAxes: [{
                              ticks: {
                                  beginAtZero: true
                              }
                          }]
                      }
                  }
                });
            </div>
          </div>

        </div>
        <!-- /.row -->

      </div>
    </section>

    <!-- Footer -->
    <footer class="footer text-center">
      <div class="container">
        <div class="row">

          <!-- Footer Location -->
          <div class="col-lg-4 mb-5 mb-lg-0">
            <h4 class="text-uppercase mb-4">Contact us</h4>
            <p class="lead mb-0">control.ottawa<i class="fas fa-at"></i>gmail.com</p>
          </div>

          <!-- Footer Social Icons -->
          <div class="col-lg-4 mb-5 mb-lg-0">
            <h4 class="text-uppercase mb-4">Around the Web</h4>
            <a class="btn btn-outline-light btn-social mx-1" href="https://www.facebook.com/groups/CitizenIG/">
              <i class="fab fa-fw fa-facebook-f"></i>
            </a>
            <a class="btn btn-outline-light btn-social mx-1" href="https://www.reddit.com/r/ottawa/">
              <i class="fab fa-reddit-alien"></i>
            </a>
          </div>

          <!-- Footer About Text -->
          <div class="col-lg-4">
            <h4 class="text-uppercase mb-4">About Occasional Transport.ca</h4>
            <p class="lead mb-0">is an informative and satirical website built to communicate the status of the Ottawa Light Rail Transit lines.</p>
            <p class="lead mt-3">Why? Because someone has to.</p> 
          </div>

        </div>
      </div>
    </footer>

    <div class="portfolio-modal modal fade" id="emailMode" tabindex="-1" role="dialog" aria-labelledby="emailModeLabel" aria-hidden="true">
      <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">
              <i class="fas fa-times"></i>
            </span>
          </button>
          <div class="modal-body text-center">
            <div class="container">
              <div class="row justify-content-center">
                <div class="col-lg-8">
                  <!-- Title -->
                  <h2 class="portfolio-modal-title text-secondary text-uppercase mb-0">Email Signup Form</h2>
                  <!-- Icon Divider -->
                  <div class="divider-custom">
                    <div class="divider-custom-line"></div>
                    <div class="divider-custom-icon">
                      <i class="fas fa-star"></i>
                    </div>
                    <div class="divider-custom-line"></div>
                  </div>
                  <!-- Modal - Text -->
                  <p class="mb-2 lead">Please select <b>LRT Updates</b> to be added to the alerts mailing list. You may select other interests to receive notifications when they become active.</p>
                          
                  <div class="row">
                    <!-- Begin Mailchimp Signup Form -->
                    <div id="mc_embed_signup">
                      <form action="https://facebook.us16.list-manage.com/subscribe/post?u=42e55b91ada0909c29255b1a5&amp;id=87b2b0a7b6" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                        <div id="mc_embed_signup_scroll">
                          
                          <h2>Subscribe to Email Alerts</h2>
                          
                          <div class="indicates-required"><span class="asterisk">*</span> indicates required</div>
                          
                          <div class="mc-field-group">
                            <label for="mce-EMAIL">Email Address  <span class="asterisk">*</span></label>
                            <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL">
                          </div>

                        <div class="mc-field-group">
                          <label for="mce-FNAME">First Name  <span class="asterisk">*</span></label>
                          <input type="text" value="" name="FNAME" class="required" id="mce-FNAME">
                        </div>

                        <div class="mc-field-group">
                          <label for="mce-LNAME">Last Name  <span class="asterisk">*</span></label>
                          <input type="text" value="" name="LNAME" class="required" id="mce-LNAME">
                        </div>

                        <div class="mc-field-group input-group">
                          <strong>Gender </strong>
                          <ul>
                            <li><input type="radio" value="Male" name="MMERGE4" id="mce-MMERGE4-0"><label for="mce-MMERGE4-0">Male</label></li>
                            <li><input type="radio" value="Female" name="MMERGE4" id="mce-MMERGE4-1"><label for="mce-MMERGE4-1">Female</label></li>
                            <li><input type="radio" value="Other" name="MMERGE4" id="mce-MMERGE4-2"><label for="mce-MMERGE4-2">Other</label></li>
                            <li><input type="radio" value="Prefer not to say" name="MMERGE4" id="mce-MMERGE4-3"><label for="mce-MMERGE4-3">Prefer not to say</label></li>
                          </ul>
                        </div>

                        <div class="mc-field-group size1of2">
                          <label for="mce-BIRTHDAY-month">Birthday  <span class="asterisk">*</span></label>
                          <div class="datefield">
                            <span class="subfield monthfield"><input class="birthday required" type="text" pattern="[0-9]*" value="" placeholder="MM" size="2" maxlength="2" name="BIRTHDAY[month]" id="mce-BIRTHDAY-month"></span> / 
                            <span class="subfield dayfield"><input class="birthday required" type="text" pattern="[0-9]*" value="" placeholder="DD" size="2" maxlength="2" name="BIRTHDAY[day]" id="mce-BIRTHDAY-day"></span> 
                            <span class="small-meta nowrap">( mm / dd )</span>
                          </div>
                        </div>
                        
                        <div class="mc-address-group">
                          <div class="mc-field-group">
                            <label for="mce-MMERGE6-addr1">Home Address  <span class="asterisk">*</span> </label>
                            <input type="text" value="" maxlength="70" name="MMERGE6[addr1]" id="mce-MMERGE6-addr1" class="required">
                          </div>
                          <div class="mc-field-group">
                            <label for="mce-MMERGE6-addr2">Address Line 2</label>
                            <input type="text" value="" maxlength="70" name="MMERGE6[addr2]" id="mce-MMERGE6-addr2">		
                          </div>
                          <div class="mc-field-group size1of2">
                            <label for="mce-MMERGE6-city">City</label>
                            <input type="text" value="" maxlength="40" name="MMERGE6[city]" id="mce-MMERGE6-city" class="required">
                          </div>
                          <div class="mc-field-group size1of2">
                            <label for="mce-MMERGE6-state">State/Province/Region</label>
                            <input type="text" value="" maxlength="20" name="MMERGE6[state]" id="mce-MMERGE6-state" class="required">
                          </div>
                          <div class="mc-field-group size1of2">
